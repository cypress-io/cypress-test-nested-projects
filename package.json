{
  "name": "cypress-test-nested-projects",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-bar": "http-server src/clients/bar/app -p 9090 -c-1",
    "start-foo": "http-server src/clients/foo/app -p 8080 -c-1",
    "test": "npm run start-foo & npm run start-bar & npm run test-foo && npm run test-bar",
    "test-foo": "cypress run --project ./src/clients/foo",
    "test-bar": "cypress run --project $PWD/src/clients/bar",
    "open-foo": "cypress open --project $PWD/src/clients/foo",
    "open-bar": "cypress open --project $PWD/src/clients/bar"
  },
  "license": "MIT",
  "devDependencies": {
    "commit-message-install": "^1.4.0",
    "cypress": "3.1.5"
  },
  "dependencies": {
    "http-server": "0.11.1"
  }
}
